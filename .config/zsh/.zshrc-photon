#
#  Configuration local to the this machine 
#
alias etcher="~/Applications/balenaEtcher-1.18.4-x64.AppImage"
alias obsidian="flatpak run md.obsidian.Obsidian &"

alias nupdate="sudo nala update"
alias nupgrade="sudo nala upgrade"
alias listup="nala list --upgradable"

alias less="batcat"

alias fd=fdfind		# Debian names fd as fdfind. This alias ensures I get the right fd 
#
# Install the syntax highlighting extension
#
source /usr/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
#
# Set up homebrew
#
eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"
